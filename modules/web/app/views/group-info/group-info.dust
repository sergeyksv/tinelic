{>"views/base_page"/}
{<main}
{#teams}

<legend>
<div class="col-md-20">

		<font size = "4" face="Times New Roman">{name}
		{@gt key=t_metrics.server.proc value=0}
			( {t_metrics.server.proc} x {@math key=t_metrics.server.mem method="round"/} Mb )
		{/gt}:&nbsp&nbsp
		<span style="white-space: nowrap;">
		<span class="text-danger">{@gt key=t_metrics.errAck.dtlActions value=0}<b>{t_metrics.errAck.dtlActions}</b>{:else}0{/gt} ( {t_metrics.errAck.actions} )</span> / <span class="text-success">{@gt key=t_metrics.errAck.dtlPages value=0}<b>{t_metrics.errAck.dtlPages}</b>{:else}0{/gt} ( {t_metrics.errAck.pages} )</span>
		</span>
		&nbsp;&nbsp;&mdash;&nbsp;&nbsp; <span style="white-space: nowrap;">
		{@gt key=t_metrics.server.r value=0}
			<span class="text-danger">{@formatnumber val=t_metrics.apdex.server type="apdex"/}</span>
		{/gt}
		{@gt key=t_metrics.client.r value=0} {@gt key=t_metrics.server.r value=0} / {/gt}
			<span class="text-success">{@formatnumber val=t_metrics.apdex.client type="apdex"/}
			</span> / <span class="text-info">{@formatnumber val=t_metrics.apdex.ajax type="apdex"/}</span>
		{/gt}
		</span>
		&nbsp;&nbsp;&mdash;&nbsp;&nbsp; <span style="white-space: nowrap;">
		{@gt key=t_metrics.server.r value=0}
			<span class="text-danger">{@formatnumber val=t_metrics.server.r type="rpm"/}</span>
		{/gt}
		{@gt key=t_metrics.client.r value=0} {@gt key=t_metrics.server.r value=0} / {/gt}
			<span class="text-success">{@formatnumber val=t_metrics.client.r type="rpm"/}
			</span> / <span class="text-info">{@formatnumber val=t_metrics.ajax.r type="rpm"/}</span>
		{/gt}
		</span>
		&nbsp;&nbsp;&mdash;&nbsp;&nbsp; <span style="white-space: nowrap;">
		{@gt key=t_metrics.server.r value=0}
			<span class="text-danger">{@formatnumber val=t_metrics.server.etu type="tm"/}</span>
		{/gt}
		{@gt key=t_metrics.client.r value=0} {@gt key=t_metrics.server.r value=0} / {/gt}
			<span class="text-success">{@formatnumber val=t_metrics.client.etu type="tm"/}
			</span> / <span class="text-info">{@formatnumber val=t_metrics.ajax.etu type="tm"/}</span>
		{/gt}
		</span>
		&nbsp;&nbsp;&mdash;&nbsp;&nbsp;	<span style="white-space: nowrap;">
		{@gt key=t_metrics.server.r value=0}
			<span class="text-danger">{@formatnumber val=t_metrics.server.e type="erate"/}</span>
		{/gt}
		{@gt key=t_metrics.client.r value=0} {@gt key=t_metrics.server.r value=0} / {/gt}
			<span class="text-success">{@formatnumber val=t_metrics.client.e type="erate"/}
			</span> / <span class="text-info">{@formatnumber val=t_metrics.ajax.e type="erate"/}</span>
		{/gt}
		</font>
</div>
</legend>

