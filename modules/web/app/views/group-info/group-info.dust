<div id="paging">
	<ul class="pagination">
		{#paging}
			{@eq key="{index}" value="1"}
				<li class="{?selected}disabled{/selected}">{^selected}<a href="?sort={sortType}&by={sortByF}&page={@math key="{currentPage}" method="subtract" operand="1"/}" class="more">{/selected}<span>Prev</span>{^selected}</a>{/selected}</li>
			{/eq}
			<li class="findActive {?selected}active{/selected}"><a href="?sort={sortType}&by={sortByF}&page={index}" class="more">{index}</a></li>
			{@eq key="{index}" value="{pageCount}"}
				<li class="{?selected}disabled{/selected}">{^selected}<a href="?sort={sortType}&by={sortByF}&page={@math key="{currentPage}" method="add" operand="1"/}" class="more">{/selected}<span>Next</span>{^selected}</a>{/selected}</li>
			{/eq}
		{/paging}
	</ul>
</div>

